<h2 class=>New book</h2>
	<%= form_for(book) do |f| %>
	<b class="input-xlarge">Title<br><%= f.text_field :title, class: 'form-control' %></br></b>
	<b class="body-new">Opinion<br><%= f.text_area :body,class: 'form-control' %></br></b>
<%= f.submit 'Create Book', class: 'col-md-12 btn btn-primary' %>
<% end %>